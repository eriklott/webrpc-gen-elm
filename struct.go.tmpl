{{- define "struct" -}}

{{- $name := .Name -}}
{{- $fields := .Fields -}}
{{- $typeMap := .TypeMap -}}

type alias {{$name}} =
    {{ range $i, $field := $fields -}}
    {{ if $i }}, {{ else }}{ {{ end -}}    
    {{ $field.Name }} : {{template "type" dict "Type" $field.Type "Optional" $field.Optional "TypeMap" $typeMap}} 
    {{ end -}}}



{{- end -}}